{
    "code-runner.saveFileBeforeRun": true,
    "code-runner.runInTerminal": true,
    "git.enableSmartCommit": true,
    "editor.fontFamily": "\"Overpass Mono\", Consolas,  'Courier New',  \"Deja Vu Sans Mono\", \"IBM Plex Mono\", Hermit-Regular,  JetBrains Mono, Slabo13px-Regular, Cascadia Code, Inconsolata, Fira Code, monospace",
    "workbench.iconTheme": "material-icon-theme",
    "git.confirmSync": false,
    "terminal.integrated.fontSize": 16,
    "files.autoSave": "afterDelay",
    "terminal.integrated.enableMultiLinePasteWarning": "auto",
    "explorer.confirmDelete": false,
    "editor.minimap.enabled": false,
    "editor.accessibilitySupport": "off",
    "editor.fontLigatures": false,
    "explorer.confirmDragAndDrop": false,
    "git.autofetch": true,
    "C_Cpp.default.cppStandard": "gnu++23",
    "C_Cpp.default.cStandard": "gnu23",
    "editor.fontVariations": false,
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "cursor.cpp.disabledLanguages": [],
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "prettier.tabWidth": 4,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "glassit.alpha": 240,
    "hediet.vscode-drawio.resizeImages": null,
    "glassit.step": 10,
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 100,
    "terminal.integrated.env.windows": {},
    "vscode-edge-devtools.browserFlavor": "Default",
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "console-ninja.featureSet": "Community",
    "C_Cpp.files.exclude": {


        "**/.vscode": true,
        "**/.vs": true
    },
    "codeium.enableConfig": {
        "*": false
    },
    "workbench.colorTheme": "Altrazen Dracula Version",
    "editor.formatOnType": true,
    "[javascriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "editor.fontSize": 13,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
